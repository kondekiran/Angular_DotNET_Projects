@model Usersdata

<form method="post" asp-action="Delete">
    <input asp-for="Id" hidden />
    <div class="border p-3 mt-4">
        <div class="row pb-2">
            <h2 class="text-primary">Delete UserData</h2>
            <hr />
        </div>
       @* <div asp-validation-summary="All"></div>*@
        <div class="mb-2">
            <label asp-for="FirstName"></label>
            <input asp-for="FirstName" class="form-control" disabled/>
          
        </div>
        <div class="mb-2">
            <label asp-for="LastName"></label>
            <input asp-for="LastName"  class="form-control" disabled/>
            
        </div>
        <div class="mb-2">
            <label asp-for="Age"></label>
            <input asp-for="Age"  class="form-control" disabled/>
           
        </div>
        <div class="mb-2">
            <label asp-for="Gender"></label>
            <input asp-for="Gender" class="form-control" disabled/>
            
        </div>
        <div class="mb-2">
            <label asp-for="Profession"></label>
            <input asp-for="Profession" class="form-control" disabled/>
            
        </div>
        <button type="submit" class="btn btn-primary" style="width:150px">Delete</button>
        <a asp-asp-controller="Userdata" asp-action="Index" class="btn btn-secondary" style="width:150px">Back to List</a>
    </div>
</form>

@section Scripts{
    @{
        <partial name="_ValidationScriptsPartial.cshtml" />
    }
}


